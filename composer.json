{
  "name": "mdavid-dev/symfony-correlation-id-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle to manage correlation IDs across HTTP requests and responses",
  "keywords": ["symfony", "bundle", "correlation-id", "request-id", "tracing"],
  "license": "MIT",
  "authors": [
    {
      "name": "mdavid-dev"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/http-foundation": "^6.4|^7.0",
    "symfony/http-kernel": "^6.4|^7.0",
    "symfony/uid": "^6.4|^7.0",
    "symfony/yaml": "^6.4|^7.0",
    "symfony/console": "^6.4|^7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "monolog/monolog": "^3.0",
    "phpunit/phpunit": "^10.0",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/phpunit-bridge": "^6.4|^7.0"
  },
  "autoload": {
    "psr-4": {
      "MdavidDev\\SymfonyCorrelationIdBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MdavidDev\\SymfonyCorrelationIdBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
